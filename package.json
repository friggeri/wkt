{
  "name": "@adrien/wkt",
  "version": "1.9.0",
  "description": "Tree Sitter grammar to describe running workouts",
  "repository": {
    "type": "git",
    "url": "https://github.com/friggeri/wkt.git"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "directories": {
    "test": "test"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tree-sitter build -w -o wasm/tree-sitter-wkt.wasm",
    "prepublishOnly": "npm run build",
    "test": "tree-sitter test",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip"
  },
  "author": "Adrien Friggeri",
  "license": "MIT",
  "devDependencies": {
    "tree-sitter-cli": "^0.22.2",
    "prebuildify": "^6.0.0"
  },
  "dependencies": {
    "node-gyp-build": "^4.8.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "Readme.md"
  ],
  "tree-sitter": [
    {
      "scope": "source.wkt",
      "file-types": [
        "wkt"
      ]
    }
  ]
}
